.page.fiche-clinique-depistage-systematique
  .contain.ng-show
    form.form-horizontal(name='form')
      .container

        //- Entête
        .row
          br
          a.btn.btn-default(role="button" href="./") Accueil
          h1 Fiche clinique - dépistage systématique :
          hr
          .container
            .row
              .col-md-5
                .form-group
                  .input-group
                    span.input-group-addon
                      span.fa.fa-id-card.fa-fw
                    input.form-control(ng-model="vm.fiche.data.patient.name", placeholder='Nom et prénom (ex: Dupont Philippe)', type='text')
              .col-md-1
              .col-md-5
                .form-group
                  .input-group
                    span.input-group-addon
                      span.fa.fa-black-tie.fa-fw
                    input.form-control(ng-model="vm.fiche.data.patient.job", placeholder='Profession (ex: Ingénieur informatique, Etudiant...)', type='text')
            .row
              .col-md-5
                .form-group
                  .input-group
                    span.input-group-addon
                      span.fa.fa-birthday-cake.fa-fw
                    input.form-control(ng-model="vm.fiche.data.patient.birthdate", placeholder='Date de naissance (ex: 28/02/1996)', type='text')
              .col-md-1
              .col-md-5
                .form-group
                  .input-group
                    span.input-group-addon
                      span.fa.fa-calendar-check-o.fa-fw
                    input.form-control(ng-model="vm.fiche.data.patient.last_visit", placeholder='Dernière consultation (ex: 12/12/2016)', type='text')
            .row
              .col-md-5
                .form-group
                  .input-group
                    span.input-group-addon
                      span.fa.fa-mars-stroke-v.fa-fw
                    select.form-control(ng-model="vm.fiche.data.patient.gender")
                      option(value='homme') Homme
                      option(value='femme') Femme
              .col-md-1
              .col-md-5
                .form-group
                  .input-group
                    span.input-group-addon
                      span.fa.fa-user-md.fa-fw
                    input.form-control(ng-model="vm.fiche.author", placeholder='Médecin prenant en charge ce patient (ex: Mme Geaugeais)', type='text')
            .row
              .col-md-5
                .form-group
                  button.btn.btn-primary(name='') Ajouter une photo
            .row.form-group
              label.col-md-4.control-label(for='bilanDeSante') Bilan de santé :
              .col-md-7
                textarea#bilanDeSante.form-control(ng-model="vm.fiche.data.patient.health_report" placeholder="Bilan de santé (ex: Patient souffrant de douleurs aux pré-molaires inférieures)")


        //- partie 1 Entretien
        .row
          h2 1. Entretien :
          .form-group
            .col-md-4
              label.radio-inline(for='1_0-0')
                input#1_0-0(type='radio' value="renseigne" ng-model="vm.data.consultation.etat")
                |       Renseigné
              label.radio-inline(for='1_0-1')
